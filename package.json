{
  "name": "genkit-node",
  "version": "1.0.0",
  "description": "Genkit Node HelloWorld",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "genkit:start": "genkit start -- npm run start",
    "genkit:start:debug": "genkit start -- npm run start:debug",
    "start": "tsx --watch src/index.ts",
    "start:debug": "tsx --watch --inspect-brk src/index.ts",
    "build": "tsc"
  },
  "author": "",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "default": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@genkit-ai/express": "^1.0.5",
    "dotenv": "^16.4.7",
    "genkit": "^1.0.5",
    "genkitx-github": "^1.13.1",
    "openweather-api-node": "^3.1.5"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
